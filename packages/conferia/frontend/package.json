{
  "name": "@bpm2025-website/conferia-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "browserslist": [
    "last 2 versions"
  ],
  "imports": {
    "#/*": "./src/*"
  },
  "exports": {
    ".": {
      "default": "./index.ts"
    },
    "./instance": {
      "default": "./src/app.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "vue-tsc --noEmit || true",
    "prod": "npm run build && npm run preview",
    "lint": "eslint . || true",
    "lint:fix": "eslint . --fix || true",
    "lint:inspect": "eslint-config-inspector",
    "generate-pwa-icons": "node generate-pwa-icons.cjs"
  },
  "dependencies": {
    "@bpm2025-website/assets": "*",
    "@primevue/themes": "4.4.0",
    "@vueuse/core": "13.9.0",
    "axios": "1.11.0",
    "dayjs": "1.11.18",
    "primevue": "4.4.0",
    "vue": "3.5.22",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@bpm2025-website/configs": "*",
    "@vitejs/plugin-vue": "6.0.1",
    "browserslist": "4.25.3",
    "lightningcss": "1.30.1",
    "unocss": "66.4.2",
    "vite": "7.1.3",
    "vite-plugin-pwa": "1.0.3",
    "vue-tsc": "3.0.6"
  }
}